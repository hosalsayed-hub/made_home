                            </div>

                            <div class="form-group col-md-6">
                                <div class="row">
                                    <input asp-for="IsAvailable" class="AddData" type="hidden" />
                                    <label class="col-xs-12">@Homemade.UI.Resources.Homemade.Product_Status</label>
                                    <div class="col-xs-12">
