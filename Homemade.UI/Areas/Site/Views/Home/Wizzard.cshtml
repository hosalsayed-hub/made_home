@inject Homemade.BLL.Setting.BlMainPageDetails blMainPageDetails
@{
    ViewData["Title"] = "Wizzard";
    Layout = "~/Views/Shared/_SiteLayout.cshtml";
}

@section Styles{
    <link href="~/assets/css/provider-page-css.css" rel="stylesheet" />

    <style>
        .controls {
            width: 250px !important;
        }

        .error {
            color: red
        }
    </style>
}

    @Html.AntiForgeryToken()

    <section class="section-style provider-apply-section">

        <div class="container">

            <div class="row">


                <div class="col-12">

                    <div class="provider-apply-area">

                        <div class="provider-apply-area-head">

                            <h1 class="provider-apply-title title-2">@Homemade.UI.Resources.Homemade.Become_a_Vendor  </h1>
                            <p class="provider-apply-head-text">@Homemade.UI.Resources.Homemade.Become_a_VendorDesc     </p>
                        </div>
                        <div class="provider-apply-steps-area">


                            <div class="wizard-btns">

                                <div class="step-btn active" data-target="terms_data" tap-id="0" id="terms_data">

                                    <div class="step-icon"><i class="fas fa-circle"></i></div>
                                    <div class="step-name"> @Homemade.UI.Resources.Homemade.Terms_and_Conditions   </div>
                                </div>
                                <div class="step-btn " data-target="personal_data" tap-id="1" id="personal_data">

                                    <div class="step-icon"><i class="fas fa-circle"></i></div>
                                    <div class="step-name"> @Homemade.UI.Resources.Homemade.Personal_Data   </div>
                                </div>
                                <div class="step-btn " data-target="store_data" tap-id="2" id="store_data">

                                    <div class="step-icon"><i class="fas fa-circle"></i></div>
                                    <div class="step-name">@Homemade.UI.Resources.Homemade.Store_Data   </div>
                                </div>
                                <div class="step-btn " data-target="payment_data" tap-id="3" id="payment_data">

                                    <div class="step-icon"><i class="fas fa-circle"></i></div>
                                    <div class="step-name"> @Homemade.UI.Resources.Homemade.PaymentType   </div>
                                </div>

                            </div>
                            <div class="steps-items tabs-content">
                                <div id="messageBlock">
                                </div>
                                <div class="step-item tab-content terms_data active " id="terms_data">
                                    <div class="step-content">
                                        <div class="row" id="formterms_data">
                                            <div class="col-12">
                                                @{

                                                var terms = blMainPageDetails.GetMainPageDetailsViewModelForTermsVendor(Utility.CurrentLanguageCode);
                                            }

                                            @if (terms != null)
                                            {
                                                <p class="accordion-content">
                                                    @Html.Raw(terms.Desc.Replace("\n", "</br>"))
                                                </p>
                                            }

                                        </div>
                                        <div class="form-check terms-checkbox">

                                            <input class="form-check-input" type="checkbox" id="IsAgreeTerms" name="IsAgreeTerms" value="1">
                                            <label class="form-check-label" for="IsAgreeTerms">
                                                @Homemade.UI.Resources.Homemade.I_agree_to
                                                <span class="terms-link">@Homemade.UI.Resources.Homemade.Terms_and_Conditions</span>
                                            </label>
                                        </div>
                                    </div>
                                </div>

                                <div class="wizard-arrow">
                                    <button type="button" class="arrow-btn next-btn " current-tab="0" onclick="nextTab('#formterms_data','personal_data')">
                                        <span class="tab_name_next">@Homemade.UI.Resources.Homemade.Personal_Data   </span>
                                        <i class="fas fa-arrow-left"></i>
                                    </button>

                                </div>
                            </div>
                            <div class="step-item tab-content personal_data " id="personal_data">
                                <div class="step-content">

                                    <form enctype="multipart/form-data" class="wizard-form" action="javascript:void(0);" id="formpersonal_data">



                                        <div class="row">
                                            <div class="col-12">

                                                <div class="form-group">
                                                    <div class="upload-files-area">
                                                        <i class="fa-redo-alt fas removebtnownerlogo d-none" onclick="RemoveImage('ownerlogo')"></i>
                                                        <button class="upload-btn" onclick="$('#fupownerlogo').click()" id="ownerlogo">
                                                            <span>
                                                                <img id="img_ownerlogo" class="img_set" style="max-width:45px;" src='data:image/svg+xml;base64,' />
                                                            </span>
                                                            @Homemade.UI.Resources.Homemade.PersonalPicture
                                                        </button>

                                                        <input type="file" id="fupownerlogo" name="fupownerlogo" class="form-control-file  validate[required] d-none" accept="image/*" onchange="ConvertToBase64(this,'img_ownerlogo')" />

                                                    </div>
                                                </div>
                                            </div>

                                            <div class="col-md-6">

                                                <div class="form-group">

                                                    <label class="form-label">@Homemade.UI.Resources.Homemade.Name   </label>
                                                    <input type="text" class="form-control input-focus" id="Txt_FnameAr" name="Txt_FnameAr" title="@Homemade.BLL.Resources.HomemadeErrorMessages.FirstNameARRequierd" required>


                                                </div>


                                            </div>
                                            <div class="col-md-6">

                                                <div class="form-group">

                                                    <label class="form-label">@Homemade.UI.Resources.Homemade.Nationality</label>
