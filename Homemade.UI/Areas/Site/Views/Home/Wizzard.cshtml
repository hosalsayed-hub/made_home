                                                        <option selected value=""></option>
                                                    </select>

                                                </div>


                                            </div>

                                            @*<div class="col-md-3">

                                            <div class="form-group">

                                            <label class="form-label">@Homemade.UI.Resources.Homemade.First_Name_EN</label>
                                            <input type="text" required class="form-control input-focus" id="Txt_FnameEn" name="Txt_FnameEn" title="@Homemade.BLL.Resources.HomemadeErrorMessages.FirstNameENRequierd">

                                            </div>


                                            </div>
                                            <div class="col-md-3">

                                            <div class="form-group">

                                            <label class="form-label">@Homemade.UI.Resources.Homemade.Family_Name_AR</label>
                                            <input type="text" class="form-control input-focus" id="Txt_SnameAr" name="Txt_SnameAr">

                                            </div>


                                            </div>
                                            <div class="col-md-3">

                                            <div class="form-group">

                                            <label class="form-label">@Homemade.UI.Resources.Homemade.Family_Name_EN</label>
                                            <input type="text" class="form-control input-focus" id="Txt_SnameEn" name="Txt_SnameEn">

                                            </div>


                                            </div>*@


