                                                <div class="form-group">

                                                    <label class="form-label">@Homemade.UI.Resources.Homemade.City</label>
                                                    <select required class="form-control js-select2" onchange="LoadBlocks(this)" id="cities" name="cities" style="width: 100%;" data-placeholder="@Homemade.UI.Resources.Homemade.Select" size="1" title="@Homemade.BLL.Resources.HomemadeErrorMessages.CityRequierd">
