<div class="custom-bootstrap-modal modal fade scheduled-request-modal" id="VerviedCodeModal" tabindex="-1" aria-labelledby="VerviedCodeModalLabel" aria-hidden="true"
     data-backdrop="static">
    <div class="modal-dialog modal-dialog-centered modal-lg">
        <div class="modal-content">
            <div class="modal-header">

                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>

            </div>
            <div class="modal-body">


                <div class="col-title title-4">@Homemade.UI.Resources.Homemade.Mobile_Number_Verification</div>

                <p class="col-text" id="Div_Loading">
                    <img src="../../assets/homeMadeSite/css/ajax-loader.gif" style="width: 50px;">
                    @Homemade.UI.Resources.Homemade.Verification_code_is_being_sent
                </p>
                <p class="col-text" id="Div_sendcode" style="display:none">
                    @Homemade.UI.Resources.Homemade.Please_enter_the_code_sent_to_your_phone
                </p>
                <div class="modal-body-content">

                    <div class="row" id="divMsgParentRegisterVendorCode" style="display:none">
                        <div class="col-xs-12">
                            <div id="divMsgClassRegisterVendorCode" class="alert alert-danger" role="alert">
                                <strong id="lblMsgRegisterVendorCode"></strong>
                            </div>
                        </div>
                    </div>
                    <form class="scheduled-request-form">

                        <div class="form-group">

                            <div class="form-group tel-field">
                                <label class="form-label">@Homemade.UI.Resources.Homemade.The_code_sent_to_you </label>
                                <input type="number" class="form-control input-focus code-input" id="VendorRegisterCode">
                            </div>

                        </div>
